static Object lock=new Object();
	@Override
	public void run() {
		
		synchronized (lock) {
			
		}
	
		
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		EvenOddInThreads eoit = new EvenOddInThreads();
		EvenOddInThreads eoit1 = new EvenOddInThreads();
		
		eoit.start();
		eoit1.start();

	}

}
